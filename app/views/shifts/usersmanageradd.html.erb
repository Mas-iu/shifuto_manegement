<h1>管理者が従業員を登録するページ</h1>
<h2>
<%= form_with url: shifts_path do |form| %>

<%#= form_with model: @shift do |from| %>
  <div class="field">
    <%= form.label :employee_name, "従業員名" %>
    <%= form.text_field :employee_name %>
  </div>

  
  <div class="field">
    <%= form.label :employee_name_kana, "ジュウギョウインメイ" %>
    <%= form.text_field :employee_name %>
  </div>

  <div class="field">
    <%= form.label :email, "E-mail" %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :user_id, "従業員ID" %>
    <%= form.text_field :user_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
</h2>
